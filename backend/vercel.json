{
  "version": 2,
  "builds": [
      {
          "src": "./backend/server.js",
          "use": "@vercel/node"
      },
      {
        "src": ".frontend/build",
        "use": "@vercel/node"
    }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/backend/server.js"
      }
  ]
}